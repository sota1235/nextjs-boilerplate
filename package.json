{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "fix": "npm run prettier && npm run eslint:fix",
    "stylelint": "stylelint './{src,.storybook}/**/*.{ts,tsx}'",
    "prettier": "prettier './{src,.storybook}/**.{ts,tsx}' --write",
    "eslint": "eslint '{src,.storybook}/**/*.{ts,tsx}'",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "npm run eslint && npm run stylelint",
    "test": "jest",
    "test:ci": "jest -c ./jest.config.ci.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "next": "^10.0.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.4",
    "redux": "4.1.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-storyshots": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^6.0.0",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "^25.2.3",
    "@types/node": "15.12.5",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/react-redux": "7.1.16",
    "@types/storybook__addon-storyshots": "5.3.0",
    "@types/storybook__react": "4.0.2",
    "@types/styled-components": "5.1.10",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "babel-core": "6.26.3",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-styled-components": "1.12.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.29.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "husky": "4.3.8",
    "jest": "^25.5.4",
    "lint-staged": "11.0.0",
    "prettier": "2.2.1",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "^4.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run fix"
    ]
  }
}
