{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "fix": "npm run prettier && npm run eslint:fix",
    "stylelint": "stylelint './{src,.storybook}/**/*.{ts,tsx}'",
    "prettier": "prettier './{src,.storybook}/**.{ts,tsx}' --write",
    "eslint": "eslint '{src,.storybook}/**/*.{ts,tsx}'",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "npm run eslint && npm run stylelint",
    "test": "jest",
    "test:ci": "jest -c ./jest.config.ci.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "next": "^12.1.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "7.2.8",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@next/eslint-plugin-next": "^12.1.6",
    "@sota1235/eslint-config": "^3.0.0",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addons": "^6.3.6",
    "@storybook/cli": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/jest": "^27.0.0",
    "@types/node": "17.0.35",
    "@types/react": "17.0.45",
    "@types/react-dom": "18.0.5",
    "@types/react-redux": "7.1.24",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "5.1.25",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^28.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-styled-components": "2.0.7",
    "core-js": "^3.8.2",
    "eslint": "8.16.0",
    "husky": "8.0.1",
    "jest": "^28.0.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "12.4.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "2.6.2",
    "stylelint": "^14.0.1",
    "stylelint-config-recommended": "7.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run fix"
    ]
  }
}
