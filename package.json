{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "fix": "npm run prettier && npm run eslint:fix",
    "stylelint": "stylelint './{src,.storybook}/**/*.{ts,tsx}'",
    "prettier": "prettier './{src,.storybook}/**.{ts,tsx}' --write",
    "eslint": "eslint '{src,.storybook}/**/*.{ts,tsx}'",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "npm run eslint && npm run stylelint",
    "test": "jest",
    "test:ci": "jest -c ./jest.config.ci.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "next": "^9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.1",
    "redux": "4.0.5",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.0.26",
    "@storybook/addon-links": "6.0.26",
    "@storybook/addon-storyshots": "6.0.26",
    "@storybook/addons": "6.0.26",
    "@storybook/react": "6.0.26",
    "@types/enzyme": "3.10.7",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.10",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/storybook__addon-storyshots": "5.1.2",
    "@types/storybook__react": "4.0.2",
    "@types/styled-components": "5.1.4",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "babel-core": "6.26.3",
    "babel-jest": "26.5.2",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-styled-components": "1.11.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.4",
    "husky": "4.3.0",
    "jest": "26.5.3",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "stylelint": "13.7.2",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "^4.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run fix"
    ]
  }
}
